@model pfeCLS_website.Models.Branche

@{
    ViewBag.Title = "DeleteBr";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<section id="main-content">
    <section class="wrapper">
        <div class="row">
            <div class="col-lg-12">
                @Html.Partial("_Notifications")
                <h3 class="page-header"><i class="icofont-ui-delete"></i>Supprimer Branche</h3>
                <ol class="breadcrumb">
                    <li><i class="icofont-home"></i><a href="@Url.Action("DeleteBr","AdminLog")">Accueil</a></li>
                    <li>Branche</li>

                </ol>
            </div>
        </div>
        <!-- Form validations -->
        <div class="row">
            <div class="col-lg-12">
                <section class="panel">
                    <header class="panel-heading">
                        Voullez-vous vraiment supprime cette branche ?
                    </header>
                    <div class="panel-body">
                        <div class="form">
                            <hr />
                            <dl class="dl-horizontal">
                                <dt>
                                    @*@Html.DisplayNameFor(model => model.Nom_branche)*@
                                    Nom :
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.Nom_branche)
                                </dd>
                                <dt>
                                    @*@Html.DisplayNameFor(model => model.Nom_branche)*@
                                    Prix :
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.prix_branche)
                                </dd>
                                <dt>
                                    @*@Html.DisplayNameFor(model => model.Nom_branche)*@
                                    Frais :
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.frais_inscri)
                                </dd>

                            </dl>

                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()

                                <div class="form-actions no-color">
                                    <input type="submit" value="Supprimer" class="btn btn-default" /> |
                                    <i class="icofont-reply"></i>@Html.ActionLink("retournez à la page précédente", "ListBranches")
                                </div>
                            }
                        </div>
                    </div>
                </section>

                <br /><br /><br /><br />
            </div>
        </div>
    </section>
</section>
